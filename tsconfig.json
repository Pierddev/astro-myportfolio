{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["../components/*"],
      "@layouts/*": ["../layouts/*"],
      "@assets/*": ["../src/assets/*"],
      "@styles/*": ["../styles/*"],
      "@icons/*": ["../components/icons/*"],
      "@data/*": ["../data/*"]
    }
  }
}